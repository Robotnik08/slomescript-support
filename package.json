{
  "name": "slomescript-vscodesupport",
  "displayName": "SlomeScript",
  "description": "Support for SlomeScript",
  "version": "1.0.1",
  "publisher": "SebastiaanHeins",
  "repository": {
    "type": "git",
    "url": "https://github.com/Robotnik08/slomescript-vscodesupport"
  },
  "icon": "icons/Slome2LogoBig.png",
  "license": "Me",
  "engines": {
    "vscode": "*"
  },
  "contributes": {
    "languages": [
      {
        "id": "slomescript",
        "extensions": [
          ".slome",".slomescript", ".sls"
        ],
        "aliases": [
          "SlomeScript",
          "sls"
        ],
        "icon": {
          "light": "icons/Slome2LogoSmall.png",
          "dark": "icons/Slome2LogoSmall.png"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "slomescript",
        "scopeName": "source.sls",
        "path": "./syntaxes/sls.tmLanguage.json"
      }
    ]
  }
}